<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
        "http://struts.apache.org/dtds/struts-2.5.dtd">

        
<struts>
  	<constant name="struts.devMode" value="true" />

   
	<package name="packageName" extends="struts-default" namespace="/">
	
	<!-- 定义action -->
		<action name="login" class="com.sjk.action.Login" method="execute">
			<result name="Success" type="redirect">/Page/CheckIn.jsp</result>
		</action>	
	
		<action name="SelectPage" class="com.sjk.action.SelectPage" method="execute">
			<result name="0" >/Page/CheckIn.jsp</result>
			<result name="1" type="redirect">/Page/ACOTime.jsp</result>
			<result name="2" type="redirect">/Page/FindInformation.jsp</result>
			<result name="3" type="redirect">/Page/Change.jsp</result>
			<result name="4" type="redirect">index.html</result>
		</action>
	
	</package>



</struts>